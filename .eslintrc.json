{
    "env": {
        "browser": false
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    },
     "globals": {
        "$": false,
        "window": false,
        "print" : false,
        "document" : false,
        "require": false,
        "requirejs": false,
        "define": false,
        "ko": false,
        "Expand": false,
        "LANG": false,
        "LOADTEMPL" : false   
    }
}