{
    "env": {
        "browser": false
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    },
     "globals": {
        "$": false,
        "window": false,
        "require": false,
        "define": false,
        "ko": false,
        "LANG": false    
    }
}